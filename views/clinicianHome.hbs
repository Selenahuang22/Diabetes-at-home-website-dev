<!-- Clinician dashboard page-->
<section id="clinicianHome">

    <!-- clinician personal information section-->
    <section id="clinicianInfo">
        <section id="directP">
            <h2><a href="/clinician/{{_id}}/profile" class="fontBlueNormal">Home</a> > <a href="" class="fontBlueNormal">Dashboard</a></h2>
        </section>
    </section>

    <!-- Clinician patients table-->
    <section id="allPatients">
            <table id="patientList">
                <tr>
                    <th>Patient List</th>
                    <th>Blood Glucose Level
                        (in mmol/L)</th>
                    <th>Weight (in kg)</th>
                    <th>Number of Insulin
                        Doses Taken</th>
                    <th>Exercises
                        (step count)</th>
                </tr>
            </table>
            <table id="tableScroll">
                <!-- Patients from the database -->
                {{#each patient}}
                
                <tr>
                    <td><a class="fontBlueNormal" href="patient/{{_id}}">{{this.first_name}} {{this.last_name}}</a></td>
                    
                    {{#if latest_log.length}}
                        {{#each latest_log}}
                            <td {{#if (is_bgl_in_dangRange this.value)}}class="fontRed"{{/if}} >{{#if (is_blood_glucose this.name)}}{{this.value}}{{else}}-{{/if}}</td>
                            <td>{{#if (is_insulin_take this.name)}}{{this.value}}{{else}}-{{/if}}</td>
                            <td>{{#if (is_weight this.name)}}{{this.value}}{{else}}-{{/if}}</td>
                            <td>{{#if (is_exercises this.name)}}{{this.value}}{{else}}-{{/if}}</td>
                        {{/each}}
                        
                    {{else}}
                        <td>-</td>
                        <td>-</td>
                        <td>-</td>
                        <td>-</td>
                    {{/if}}
                    
                </tr>
                
                {{/each}}

