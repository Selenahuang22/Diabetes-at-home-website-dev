                    <th>Exercises
                        (step count)</th>
                </tr>

                {{#each patient}}
                
                <tr>
                    <td class="fontBlueNormal">{{this.first_name}} {{this.last_name}}</td>
                    
                    {{#if latest_log.length}}
                        {{#each latest_log}}
