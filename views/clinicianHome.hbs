                            <td>{{#if (is_weight this.name)}}{{this.value}}{{else}}-{{/if}}</td>
                            <td>{{#if (is_exercises this.name)}}{{this.value}}{{else}}-{{/if}}</td>
                        {{/each}}
                        
                    {{else}}
                        <td>-</td>
                        <td>-</td>
